{
  "manifest_version": 2,
  "name": "URL Saver",
  "version": "1.0",
  "description": "A simple extension to save URLs with categories",
  "browser_action": {
    "default_popup": "public/popup.html", 
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab"
  ]
}
